{
    "name" : "PWrSantaMode",
    "description" : "What about xmas",
    "version" : "1.0.0",
    "content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["Content.js"]
		}
	],
    "manifest_version": 3,
    "permissions": ["storage", "tabs"],
    "action": {
        "default_popup": "popup.html"
        
    },
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["images/gownoDobre.gif"]
    }]
}